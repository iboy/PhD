	Identifier = {01917564},
	Issn = {1049-8907},
	Journal = {The Journal of Visualization \& Computer Animation},
	Msc2010 = {I.3.7 (H.5.2)},
	Number = {4},
	Pages = {169-183},
	Publisher = {John Wiley \& Sons Ltd, Chichester},
	Title = {Evolving performance control systems for digital puppetry.},
